{
    "name": "P12",
    "title": "Platform P12 (LGT8F)",
    "description": "Logic Green AVR MCU LGT8F series",
    "homepage": "http://www.OS-Q.com/P12",
    "version": "1.0.5",
    "engines": {
        "platformio": "^5"
    },
    "keywords": [
        "OS-Q",
        "dev-platform",
        "avr",
        "lgt8fx",
        "MCU",
        "arduino",
        "8-bit"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/OS-Q/P12.git"
    },
    "frameworks": {
        "arduino": {
            "package": "A12",
            "script": "extend/arduino.py"
        },
        "native": {
            "package": "E12A",
            "script": "extend/native.py"
        }
    },
    "packages": {
        "A12": {
            "type": "framework",
            "optional": true,
            "owner": "OS-Q",
            "version": "https://github.com/OS-Q/A12.git"
        },
        "E12A": {
            "type": "framework",
            "optional": true,
            "owner": "OS-Q",
            "version": "https://github.com/OS-Q/E12A.git"
        },
        "toolchain-atmelavr": {
            "type": "toolchain",
            "version": "~1.70300.0"
        },
        "tool-avrdude": {
            "type": "uploader",
            "version": "~1.60300.0"
        }
    }
}
